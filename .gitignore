# Laravel .gitignore for VMS Project

# Environment files (NEVER commit these!)
.env
.env.backup
.env.production
.env.local

# Dependencies
/node_modules
/vendor

# Laravel specific
/bootstrap/cache/*.php
/storage/*.key
/storage/logs/*.log
/storage/app/*
/storage/framework/cache/*
/storage/framework/sessions/*
/storage/framework/views/*
!/storage/app/.gitkeep
!/storage/app/public/.gitkeep

# Laravel Mix / Vite
/public/hot
/public/storage
/public/css/app.css
/public/js/app.js
/public/mix-manifest.json

# IDE files
.vscode/
.idea/
*.swp
*.swo
*~

# OS files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# Python ML files (if any)
__pycache__/
*.py[cod]
*$py.class
*.pkl
*.joblib

# Logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Cache
.cache
.temp

# Database
*.sqlite
*.sqlite3

# Backup files
*.bak
*.backup

# Temporary files
*.tmp
*.temp

# Don't commit sensitive ML model files
/model_training_output/*.pkl
/python/__pycache__/